{
  "api": {
    "host": "sensu-api",
    "port": 4567
  },
  "handlers": {
    "default": {
      "type": "pipe",
      "command": "cat"
    },
    "metrics": {
      "type": "set",
      "handlers": ["influxdb-extension"]
    },
    "events": {
      "type": "set",
      "handlers": ["influxdb-proxy-extension"]
    },
    "events_nano": {
      "type": "set",
      "handlers": ["influxdb-proxy-extension-nano"]
    }
  }
}
